<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.cots">

    <application
        android:allowBackup="true"
        android:icon="@drawable/cotsl_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/cotsl_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="activities.article_detaiil"></activity>
        <activity android:name="activities.Main4Activity" />
        <activity
            android:name="activities.Main3Activity"
            android:label="@string/title_activity_main3"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="activities.create_account" />
        <activity android:name="activities.login" />
        <activity
            android:name="activities.full_image"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="activities.Main2Activity" />
        <activity android:name="activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="activities.full_image"
            android:theme="@style/AppTheme.NoActionBar"
            tools:ignore="DuplicateActivity" />
        <activity
            android:name="activities.post_full_image"
            android:theme="@style/AppTheme.NoActionBar"
            tools:ignore="DuplicateActivity" />

        <service android:name="activities.MyMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>